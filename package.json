{
  "name": "@vayu/nasa-sdk",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run test && npm run build"
  },
   "repository": {
    "type": "git",
    "url": "git+https://github.com/anand-1809/nasa-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/anand-1809/nasa-sdk/issues"
  },
  "homepage": "https://github.com/anand-1809/nasa-sdk#readme",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  }
}
